<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dhu.pojo.TbBookinfo" table="tb_bookinfo" schema="library">
        <id name="id" column="id"/>
        <property name="bookid" column="bookid"/>
        <property name="bookname" column="bookname"/>
        <property name="type" column="type"/>
        <property name="author" column="author"/>
        <property name="press" column="press"/>
        <property name="allamount" column="allamount"/>
        <property name="amount" column="amount"/>
        <property name="price" column="price"/>
        <property name="remark" column="remark"/>
    </class>
</hibernate-mapping>